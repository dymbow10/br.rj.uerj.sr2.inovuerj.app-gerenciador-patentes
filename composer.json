{
  "name": "emiliojva/inovuerj_intellectual",
  "description": "MicroFramework para atender demandas de projetos curtos, do Departamento de Inovação da UERJ. Inovuerj",
  "homepage": "http://inovuerj.sr2.uerj.br",
  "authors": [
    {
      "name": "Emilio Vieira",
      "email": "emilio@sr2.uerj.br"
    }
  ],
  "config": {
    "sort-packages": true,
    "platform": {
        "php": "5.6.4"
    }
  },
  "require": {
    "php": ">=5.6",
    "firebase/php-jwt": "dev-master",
    "illuminate/database": "^5.4",
    "illuminate/events": "^5.4",
    "league/plates": "3.3.*",
    "monolog/monolog": "1.*",
    "php-curl-class/php-curl-class": "8.9.3",
    "psr/http-message": "^1.0",
    "shuchkin/simplexlsx": "dev-master",
    "symfony/var-dumper": "^3.4"
  },
  "autoload": {
    "psr-4": {
      "App\\":"App/",
      "Core\\":"Core/",
      "Config\\":"Config/"
    }
  },
  "require-dev": {
  }
}
